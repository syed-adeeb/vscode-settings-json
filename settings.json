{
	"workbench.startupEditor": "none",
	"tabnine.experimentalAutoImports": true,
	"editor.mouseWheelZoom": true,
	"workbench.colorCustomizations": {
		"editorCursor.foreground": "#0088ff"
	},
	"code-runner.saveAllFilesBeforeRun": true,
	"editor.fontLigatures": true,
	"editor.fontFamily": "'cascadia code' ,'Hack Nerd Font'   ,Consolas, 'Courier New', monospace",
	"workbench.iconTheme": "material-icon-theme",
	"editor.fontWeight": "500",
	"explorer.confirmDelete": false,
	"security.workspace.trust.untrustedFiles": "open",
	"editor.tabCompletion": "onlySnippets",
	"emmet.triggerExpansionOnTab": true,
	"terminal.integrated.shellIntegration.enabled": true,
	// "C_Cpp.updateChannel": "Insiders",
	"workbench.experimental.layoutControl.enabled": true,
	"window.title": "${dirty}${rootName} - By Syed Adeeb",
	"better-comments.highlightPlainText": true,
	"code-runner.executorMap": {
		"javascript": "node",
		"java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
		"c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"php": "php",
		"python": "python -u",
		"perl": "perl",
		"perl6": "perl6",
		"ruby": "ruby",
		"go": "go run",
		"lua": "lua",
		"groovy": "groovy",
		"powershell": "powershell -ExecutionPolicy ByPass -File",
		"bat": "cmd /c",
		"shellscript": "bash",
		"fsharp": "fsi",
		"csharp": "cd $dir && dotnet run $filename",
		"vbscript": "cscript Nologo",
		"typescript": "ts-node",
		"coffeescript": "coffee",
		"scala": "scala",
		"swift": "swift",
		"julia": "julia",
		"crystal": "crystal",
		"ocaml": "ocaml",
		"r": "Rscript",
		"applescript": "osascript",
		"clojure": "lein exec",
		"haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
		"rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
		"racket": "racket",
		"scheme": "csi -script",
		"ahk": "autohotkey",
		"autoit": "autoit3",
		"dart": "dart",
		"pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
		"d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
		"haskell": "runhaskell",
		"nim": "nim compile --verbosity:0 --hints:off --run",
		"lisp": "sbcl --script",
		"kit": "kitc --run",
		"v": "v run",
		"sass": "sass --style expanded",
		"scss": "scss --style expanded",
		"less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
		"FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
	},
	"explorer.confirmDragAndDrop": false,
	"editor.suggestSelection": "first",
	"code-runner.runInTerminal": true,
	"editor.autoClosingQuotes": "always",
	"workbench.editorAssociations": {
		"*.sqlite3": "default",
		"*.exe": "default",
		"*": "uwu.env"
	},
	"importCost.debug": true,
	"emmet.includeLanguages": { "javascript": "javascriptreact" },
	"editor.formatOnSave": true,
	"editor.unicodeHighlight.invisibleCharacters": false,
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.lineHeight": 19,
	"emmet.showSuggestionsAsSnippets": true,
	"tailwindCSS.emmetCompletions": true,
	"editor.bracketPairColorization.enabled": true,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"emmet.syntaxProfiles": {},
	"editor.definitionLinkOpensInPeek": true,
	"html.format.indentInnerHtml": true,
	"html.format.indentHandlebars": true,
	"prettier.vueIndentScriptAndStyle": true,
	"editor.wrappingIndent": "deepIndent",
	"pythonIndent.trimLinesWithOnlyWhitespace": true,
	"prettier.useTabs": true,
	"editor.stickyTabStops": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"editor.multiCursorMergeOverlapping": false,
	"notebook.navigation.allowNavigateToSurroundingCells": false,
	"[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnPaste": true,
	"files.associations": { "*.css": "tailwindcss", "*.html": "html" },
	"editor.linkedEditing": true,
	"javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
	"javascript.format.placeOpenBraceOnNewLineForFunctions": true,
	"typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
	"typescript.format.placeOpenBraceOnNewLineForFunctions": true,
	"markdown.preview.breaks": true,
	"quokka.compactMessageOutput": true,
	"quokka.codeAutoPlayDelay": 1,
	"testing.automaticallyOpenPeekViewDuringAutoRun": true,
	"terminal.integrated.enableMultiLinePasteWarning": false,
	"search.quickOpen.includeSymbols": true,
	"terminal.integrated.allowMnemonics": true,
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.defaultProfile.linux": "",
	"terminal.integrated.tabs.description": "${task}",
	"terminal.integrated.tabs.title": "${local}",
	"tabnine.receiveBetaChannelUpdates": true,
	"editor.minimap.enabled": false,
	"reactSnippets.settings.prettierEnabled": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"gitlens.advanced.messages": { "suppressGitDisabledWarning": true },
	"git.autofetch": true,
	"gitlens.gitCommands.skipConfirmations": [
		"fetch:command",
		"stash-push:command",
		"switch:command",
		"push:command"
	],
	"extensions.ignoreRecommendations": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"files.autoSave": "afterDelay",
	"files.autoSaveDelay": 5000,
	"workbench.sideBar.location": "right",
	"window.zoomLevel": 1,
	"editor.guides.bracketPairs": "active",
	"bracket-pair-colorizer-2.depreciation-notice": false,
	"[python]": {
		"editor.formatOnType": true
	},
	"cSpell.userWords": [
		"Adeeb",
		"antdesign",
		"arrowright",
		"bieber",
		"Carrers",
		"codewithharry",
		"Esports",
		"feelslike",
		"Figtree",
		"gapi",
		"ggpht",
		"googleusercontent",
		"groq",
		"HEADSTORE",
		"heroicons",
		"hotspot",
		"hyperx",
		"Immer",
		"Ivfx",
		"localtime",
		"navigations",
		"NEXTAUTH",
		"Nextdotjs",
		"nextjs",
		"precip",
		"prismadb",
		"Prokxi",
		"Prokximus",
		"rapidapi",
		"reduxjs",
		"rockerz",
		"subt",
		"Supa",
		"Supabase",
		"Syed",
		"tailwindcss",
		"uuidv",
		"weatherapi",
		"webm",
		"webp",
		"ytimg",
		"zustand"
	],
	"prettier.bracketSameLine": true,
	"editor.cursorSurroundingLines": 1,
	"terminal.integrated.cursorStyle": "underline",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.cursorSurroundingLinesStyle": "all",
	"editor.cursorBlinking": "phase",
	"editor.cursorWidth": 2,
	"editor.quickSuggestions": {
		"other": true,
		"comments": true,
		"strings": false
	},
	"workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
	"lldb.suppressUpdateNotifications": true,
	"tabnineSelfHostedUpdater.serverUrl": "http://localhost:3000"
}
